#!/bin/bash

if [ $n1 -gt $n2 ] && [ $n1 -gt $n3 ];then

	echo "$n1 is biggest 1 number"
elif [ $n2 -gt $n1 ] && [ $n2 -gt $n3 ];
then
	echo "$n2 is biggest 2 number"
elif [ $n3 -gt $n1 ] && [ $n3 -gt $n2 ];
then
	echo "$n3 is biggest 3 number"
elif [ $1 -eq $n2 ] && [ $n1 -eq $n2 ] && [ $n1 -eq $n3 ];
then
	echo "All the three numbers are equal"
else
	echo "I can not figure out which one is biggest"
fi

